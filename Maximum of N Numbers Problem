#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,i,l,max;
    scanf("%d",&n);
    scanf("%d",&max);
    for(i=1;i<=n;i++){
        scanf("%d",&l);
        if (l>max){
            max=l;
        }
    }
    printf("%d",max);
        
    return 0;
}
