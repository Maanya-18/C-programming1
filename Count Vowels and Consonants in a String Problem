#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char x[10];
    int vowels = 0, consonants = 0;

    
    scanf("%[^\n]", x);

    for (int i = 0; x[i] != '\0'; i++) {
        char ch = tolower(x[i]); 

        if (isalpha(ch)) {
            if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u')
                vowels++;
            else
                consonants++;
        }
    }

    printf("%d %d\n", vowels, consonants);  
    return 0;
}
