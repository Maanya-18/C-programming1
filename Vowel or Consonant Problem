#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>
int main() {

    char ch;
    scanf("%c",&ch);
    if(isalpha(ch))
        ch=toupper(ch);
    
       
    if(ch =='A'|| ch =='I' || ch =='E'|| ch =='O'|| ch =='U' )
        printf("Vowel\n");
    
    else if ( !isalpha(ch) )
          printf("Invalid Input\n");
    
     else 
         printf("Consonant");
     
       
    return 0;
}
