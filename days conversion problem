#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int days,weeks,years;
    scanf("%d ",&days);
    years = days/365;
    weeks = days%365;
    weeks = weeks/7;
    days = weeks/7;
    days = days%7;
    printf("Years: %d\nWeeks: %d\nDays: %d", years,weeks,days);
    return 0;
}
