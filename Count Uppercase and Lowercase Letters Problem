#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char x[1001];
    int uppercase = 0, lowercase = 0;

    
    scanf("%[^\n]", x);

    for (int i = 0; x[i] != '\0'; i++) {
        if (isupper(x[i]))      
            uppercase++;
        else if (islower(x[i])) 
            lowercase++;
    }

    printf("%d %d\n", uppercase, lowercase);   
    return 0;
}
