#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   char x[10];
    int count = 0; 
 
        scanf("%[^\n]", x);

    for (int i = 0; x[i] != '\0'; i++) {
        char ch = tolower(x[i]);
        if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
            count++;
        }
    }

    printf("%d\n", count);
    
    return 0;
}
