#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   char x[10];
    int alpha = 0, digits = 0, special = 0;

    scanf("%[^\n]", x);

    for (int i = 0; x[i] != '\0'; i++) {
        if (isalpha(x[i]))       
            alpha++;
        else if (isdigit(x[i])) 
            digits++;
        else                       
            special++;
    }

    printf("%d %d %d\n", alpha, digits, special);  
    return 0;
}
