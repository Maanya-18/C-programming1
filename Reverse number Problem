#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,l,rev=0;
    scanf("%d",&n);
    if(n>0 && n<=9){
        printf("%d",n);
    }
    else{
        while(n!=0){
            l=n%10;
            rev=rev*10+l;
            n=n/10;
        }
    }
         printf("%d", rev);

    return 0;
}
