#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    
    int n;
    scanf("%d", &n);
    int a1[n], a2[n];
    int *a, *b;
    for(int i=0; i<n; i++){
        scanf("%d", &a1[i]);
    }
    for(int i=0; i<n; i++){
        scanf("%d", &a2[i]);
    }
    for(int i=0; i<n; i++){
        a = &a1[i];
        b = &a2[i];
        *a = *a + *b;
        *b = *a - *b;
        *a = *a - *b;
    }
    for(int i=0; i<n; i++){
        printf("%d ", a1[i]);
    }
    printf("\n");
    for(int i=0; i<n; i++){
        printf("%d ", a2[i]);
    }
    
    return 0;
}
